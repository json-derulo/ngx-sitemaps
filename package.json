{
	"name": "ngx-sitemaps",
	"version": "0.2.0",
	"description": "Generate sitemaps from Angular prerendered routes",
	"main": "./src/index.js",
	"type": "module",
	"scripts": {
		"prebuild": "node ./clean-dist.js",
		"build": "tsc",
		"postbuild": "cpy README.md dist",
		"check-code-style": "prettier . --check"
	},
	"keywords": [
		"angular",
		"sitemap"
	],
	"exports": {
		".": "./src/index.js"
	},
	"bin": {
		"ngx-sitemaps": "src/cli.js"
	},
	"author": {
		"name": "Daniel Kimmich",
		"email": "json-derulo@outlook.com"
	},
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/json-derulo/ngx-sitemaps.git"
	},
	"homepage": "https://json-derulo.github.io/ngx-sitemaps/",
	"bugs": "https://github.com/json-derulo/ngx-sitemaps/issues",
	"funding": "https://github.com/sponsors/json-derulo",
	"dependencies": {
		"commander": "^14.0.2",
		"xmlbuilder2": "^4.0.3"
	},
	"devDependencies": {
		"@types/node": "^24.10.4",
		"cpy-cli": "^6.0.0",
		"prettier": "^3.7.4",
		"typescript": "~5.9.3"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "pnpm@10.26.2"
}
